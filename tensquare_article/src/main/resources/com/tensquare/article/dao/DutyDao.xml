<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tensquare.article.dao.DutyDao">
<insert id="addDuty" parameterType="com.tensquare.article.pingan.Duty">
    insert into tb_duty(duty_id,duty_name,work_company,id_card,pay_start,pay_end,apply_date,duty_subtract_amount,
                         audit_date,ask_date,ask_result,apply_audit_days,audit_status,deal_date,duty_status,
                         audit_deal_days,deal_status,salary_count,tax_pay_amount,free_duty_amount,free_duty_name)values
                         (#{duityId},#{dutyName},#{workCompany},#{idCard},#{payStart},#{payEnd},#{applyDate},#{dutySubtractAmount},
                         #{auditDate},#{askDate},#{askResult},#{applyAuditDays},#{auditStatus},#{dealDate},#{dutyStatus},
                         #{auditDealDays},#{dealStatus},#{salaryCount},#{taxPayAmount},#{freeDutyAmount},#{freeDutyName})
</insert>
</mapper>